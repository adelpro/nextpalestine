{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "analyze": "cross-env ANALYZE=true next build",
    "lint": "next lint",
    "lint:fix": "npm run format && npm run lint -- --fix",
    "type-check": "tsc --noEmit",
    "madge": "madge --image graph.png --extensions ts src",
    "format": "prettier --write \"src/**/*.ts\"",
    "dev": "cross-env NODE_ENV=development next dev -p 3540",
    "dev-https": "cross-env NODE_ENV=development next dev --experimental-https",
    "build": "next build",
    "start": "next start -p 3540",
    "build:prod": "cross-env NODE_ENV=production next build",
    "build:dev": "cross-env NODE_ENV=development next build",
    "start:prod": "cross-env NODE_ENV=production next start",
    "start:dev": "cross-env NODE_ENV=development next start"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.5.3",
    "@lexical/code": "^0.14.5",
    "@lexical/link": "^0.14.5",
    "@lexical/list": "^0.14.5",
    "@lexical/markdown": "^0.14.5",
    "@lexical/react": "^0.14.5",
    "@lexical/utils": "^0.14.5",
    "@next/bundle-analyzer": "^13.5.6",
    "@tailwindcss/forms": "^0.5.4",
    "@tanstack/react-query": "^4.36.1",
    "@tanstack/react-query-devtools": "^4.36.1",
    "autoprefixer": "10.4.14",
    "axios": "^1.4.0",
    "clientjs": "^0.2.1",
    "clsx": "^2.0.0",
    "cross-env": "^7.0.3",
    "date-fns": "^2.30.0",
    "editorjs-html": "^3.4.3",
    "editorjs-style": "^3.0.3",
    "eslint": "^8.0.1",
    "eslint-config-next": "^14.2.3",
    "exifreader": "^4.22.1",
    "lexical": "^0.14.5",
    "list": "^2.0.19",
    "next": "^14.2.3",
    "next-pwa": "^5.6.0",
    "next-view-transitions": "^0.1.0",
    "nextjs": "^0.0.3",
    "nextjs-toploader": "^1.6.11",
    "nodemailer": "^6.9.7",
    "npm": "^10.7.0",
    "postcss": "8.4.31",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.4.1",
    "react-text-mask": "^5.5.0",
    "recoil": "^0.7.7",
    "recoil-persist": "^5.1.0",
    "sharp": "^0.32.4",
    "tailwind-merge": "^1.13.2",
    "tailwindcss": "3.3.2",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "5.1.6",
    "unsplash-js": "^7.0.19"
  },
  "devDependencies": {
    "@tanstack/eslint-plugin-query": "^4.38.0",
    "@types/clientjs": "^0.2.2",
    "@types/cookie": "^0.6.0",
    "@types/node": "^20.4.1",
    "@types/react": "^18.2.77",
    "@types/react-dom": "^18.2.24",
    "@types/react-text-mask": "^5.4.13",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "prettier-plugin-sort-imports": "^1.8.5",
    "prettier-plugin-tailwindcss": "^0.4.1"
  }
}
